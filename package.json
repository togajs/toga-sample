{
  "name": "toga-sample",
  "version": "0.1.0",
  "description": "A sample formatter for Toga documentation.",
  "keywords": [
    "togaplugin",
    "formatter",
    "example",
    "iframe",
    "sample"
  ],
  "homepage": "https://github.com/togajs/toga-sample",
  "bugs": "https://github.com/togajs/toga-sample/issues",
  "license": "MIT",
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "main": "dist/toga-sample.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/togajs/toga-sample.git"
  },
  "dependencies": {
    "karma": "^0.13.9",
    "mtil": "^0.1.3",
    "through2": "^2.0.0",
    "traverse": "^0.6.6",
    "trifle": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-istanbul": "^0.3.19",
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "expect": "^1.9.0",
    "mocha": "^2.3.0",
    "nodemon": "^1.4.1",
    "toga": "^1.0.0",
    "tunic": "^1.0.0",
    "vinyl-fs": "^1.0.0"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "build-sample": "babel src/toga-sample.js --out-file dist/toga-sample.js",
    "build-static": "cp -R src/assets dist/.",
    "build": "npm run build-sample && npm run build-static",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src/*.js test/*.js",
    "test": "babel-node node_modules/.bin/babel-istanbul cover --report text --report lcov --root src node_modules/.bin/_mocha -- 'test/*.js'",
    "watch": "nodemon --exec 'npm run test' --watch src --watch test"
  }
}
